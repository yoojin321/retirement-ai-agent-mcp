# 은퇴 설계 AI 에이전트 환경 변수 설정

# 애플리케이션 설정
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=True

# 데이터베이스 설정
POSTGRES_PASSWORD=retirement_pass
DATABASE_URL=postgresql://retirement_user:retirement_pass@postgres:5432/retirement_db

# Redis 설정
REDIS_URL=redis://redis:6379/0

# 보안 설정
SECRET_KEY=your-secret-key-here
ENCRYPTION_KEY=your-encryption-key-here

# 외부 API 설정
OPENAI_API_KEY=your-openai-api-key
FINANCIAL_DATA_API_KEY=your-financial-api-key

# MCP 서버 설정
MCP_SERVER_HOST=0.0.0.0
MCP_SERVER_PORT=8000

# 로그 설정
LOG_FILE_PATH=/app/logs/retirement-ai-agent.log
MAX_LOG_SIZE=10MB
BACKUP_COUNT=5
